{
  "name": "pwa-starter",
  "version": "0.0.1",
  "description": "A starter kit for building PWAs!",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rimraf dist/ && tsc && rollup -c",
    "build:watch": "tsc --watch",
    "build:dev": "tsc",
    "start": "es-dev-server --app-index index.html --node-resolve --watch --open",
    "dev": "concurrently \"tsc --watch\" \"es-dev-server --app-index index.html --compatibility none --node-resolve --watch --open\""
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@pwabuilder/pwainstall": "^1.6.6",
    "@vaadin/router": "^1.7.2",
    "idb-keyval": "^3.2.0",
    "lit-element": "^2.3.1",
    "marked": "^1.0.0",
    "mdn-browser-compat-data": "^1.0.21"
  },
  "devDependencies": {
    "@babel/compat-data": "^7.10.0",
    "@rollup/plugin-strip": "^1.3.2",
    "@surma/rollup-plugin-off-main-thread": "^1.4.1",
    "concurrently": "^5.2.0",
    "es-dev-server": "^1.55.0",
    "lit-analyzer": "^1.1.10",
    "rimraf": "^3.0.2",
    "rollup": "^2.18.0",
    "rollup-plugin-babel-minify": "^10.0.0",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-minify-html-literals": "^1.2.4",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-workbox": "^5.0.1",
    "typescript": "^3.9.5"
  }
}
